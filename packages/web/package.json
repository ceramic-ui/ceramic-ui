{
  "name": "@ceramic-ui/web",
  "version": "0.9.0",
  "description": "React UI Toolkit",
  "homepage": "https://github.com/ceramic-ui/ceramic-ui",
  "bugs": {
    "url": "https://github.com/ceramic-ui/ceramic-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ceramic-ui/ceramic-ui.git"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "ui",
    "styled-components"
  ],
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "styled-components": "^2.1.2"
  },
  "scripts": {
    "build:main": "BABEL_ENV=production babel src -d . --source-maps",
    "build:umd": "BABEL_ENV=umd babel src -d umd --source-maps",
    "build:docs": "build-storybook -o docs",
    "build": "run-p build:main build:umd build:docs",
    "prepublish": "run-s lint test build",
    "start": "start-storybook -p 9009",
    "test": "jest --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint -c .eslintrc.js src",
    "prettier": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.6",
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-links": "^3.2.6",
    "@storybook/addon-storyshots": "^3.2.8",
    "@storybook/react": "^3.2.8",
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.0.0",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "downshift": "^1.6.0",
    "eslint": "^4.7.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^21.0.1",
    "npm-run-all": "^4.1.1",
    "plop": "^1.8.1",
    "prettier": "^1.7.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "styled-components": "^2.1.2"
  }
}
